<p id="notice"><%= notice %></p>

<h1>Listing Vote Schedules</h1>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Date from</th>
      <th>Date to</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_schedules.each do |vote_schedule| %>
      <tr>
        <td><%= vote_schedule.date_from %></td>
        <td><%= vote_schedule.date_to %></td>
        <td><%= link_to 'Show', vote_schedule %></td>
        <td><%= link_to 'Edit', edit_vote_schedule_path(vote_schedule) %></td>
        <td><%= link_to 'Destroy', vote_schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote schedule', new_vote_schedule_path %>
